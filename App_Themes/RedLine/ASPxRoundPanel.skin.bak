<%@ Register TagPrefix="dx" Namespace="DevExpress.Data" Assembly="DevExpress.Data.v9.3, Version=9.3.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" %>
<%@ Register TagPrefix="dxrp" Namespace="DevExpress.Web.ASPxRoundPanel" Assembly="DevExpress.Web.v9.3, Version=9.3.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" %>
<dxrp:ASPxRoundPanel runat="server" Width="200px" CssPostfix="RedLine" BackColor="#F3F3F3" CssFilePath="~/App_Themes/RedLine/{0}/styles.css">
    <Border BorderStyle="None" />
    <ContentPaddings PaddingBottom="12px" />
    <HeaderStyle>
        <Border BorderStyle="None" />
    </HeaderStyle>
    <%-- Corner parts --%>
    <TopLeftCorner Height="6px" Width="6px" Url="~/App_Themes/RedLine/Web/rpTopLeftCorner.png" />
    <NoHeaderTopLeftCorner Height="6px" Width="6px" Url="~/App_Themes/RedLine/Web/rpNoHeaderTopLeftCorner.png" />
    <TopRightCorner Height="6px" Width="6px" Url="~/App_Themes/RedLine/Web/rpTopRightCorner.png" />
    <NoHeaderTopRightCorner Height="6px" Width="6px" Url="~/App_Themes/RedLine/Web/rpNoHeaderTopRightCorner.png" />
    <BottomLeftCorner Height="6px" Width="6px" Url="~/App_Themes/RedLine/Web/rpBottomLeftCorner.png" />
    <BottomRightCorner Height="6px" Width="6px" Url="~/App_Themes/RedLine/Web/rpBottomRightCorner.png" />
    <%-- Header parts --%>
    <HeaderLeftEdge>
        <BackgroundImage Repeat="NoRepeat" VerticalPosition="bottom" ImageUrl="~/App_Themes/RedLine/Web/rpHeaderLeftEdge.png" />
    </HeaderLeftEdge>
    <HeaderContent BackColor="#DADADA">
        <BackgroundImage Repeat="RepeatX" VerticalPosition="bottom" ImageUrl="~/App_Themes/RedLine/Web/rpHeaderContent.png" />
    </HeaderContent>
    <HeaderRightEdge>
        <BackgroundImage Repeat="NoRepeat" VerticalPosition="bottom" ImageUrl="~/App_Themes/RedLine/Web/rpHeaderRightEdge.png" />
    </HeaderRightEdge>
    <%-- Middle parts --%>
    <LeftEdge>
        <BackgroundImage ImageUrl="~/App_Themes/RedLine/Web/rpLeftEdge.png"></BackgroundImage>
    </LeftEdge>
    <BottomEdge>
        <BackgroundImage Repeat="RepeatX" ImageUrl="~/App_Themes/RedLine/Web/rpBottomEdge.png"></BackgroundImage>
    </BottomEdge>
    <RightEdge>
        <BackgroundImage Repeat="RepeatY" ImageUrl="~/App_Themes/RedLine/Web/rpRightEdge.png"></BackgroundImage>
    </RightEdge>
    <TopEdge>
        <BackgroundImage ImageUrl="~/App_Themes/RedLine/Web/rpTopEdge.png"></BackgroundImage>
    </TopEdge>
    <NoHeaderTopEdge BackColor="#F3F3F3" />
</dxrp:ASPxRoundPanel>
