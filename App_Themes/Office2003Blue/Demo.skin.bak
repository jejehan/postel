	</BottomEdge>
	<BottomLeftCorner Height="8px" Url="~/App_Themes/Office2003Blue/Demo/RoundPanelNavigation/rpBottomLeftCorner.png" Width="8px" />
	<RightEdge BackColor="#C8DCF7">
		<BackgroundImage HorizontalPosition="right" ImageUrl="~/App_Themes/Office2003Blue/Demo/RoundPanelNavigation/rpContentRightEdge.gif"
		Repeat="RepeatY" />
